{
  "name": "@google-cloud/cloud-sql-connector",
  "version": "1.1.0",
  "description": "A JavaScript library for connecting securely to your Cloud SQL instances",
  "author": "Google Inc.",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/mjs/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "connector",
    "cloud",
    "cloud-sql",
    "databases",
    "db",
    "Google Cloud",
    "mysql2",
    "pg",
    "sql",
    "tedious"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc -p tsconfig.json && tsc -p tsconfig-esm.json",
    "prepare": "rm -rf dist && npm run compile && node ./scripts/fixup.cjs",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "test": "tap",
    "presystem-test": "npm run prepare",
    "system-test": "tap --no-coverage system-test",
    "lint": "gts check",
    "fix": "gts fix",
    "posttest": "npm run lint"
  },
  "tap": {
    "serial": [
      "test/serial"
    ],
    "show-full-coverage": true
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "@types/semver": "^7.5.5",
    "@types/tap": "^15.0.11",
    "@types/tedious": "^4.0.14",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "c8": "^8.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "gts": "^5.2.0",
    "mysql2": "^3.6.3",
    "nock": "^13.3.8",
    "pg": "^8.10.0",
    "semver": "^7.5.1",
    "tap": "^18.5.8",
    "tedious": "^16.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleCloudPlatform/cloud-sql-nodejs-connector"
  },
  "dependencies": {
    "@googleapis/sqladmin": "^11.0.0",
    "gaxios": "^6.1.1",
    "google-auth-library": "^9.2.0",
    "mysql": "^2.18.1",
    "p-throttle": "^5.1.0"
  }
}
