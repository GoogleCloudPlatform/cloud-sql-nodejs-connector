{
  "name": "@google-cloud/cloud-sql-connector",
  "version": "0.3.0",
  "description": "A JavaScript library for connecting securely to your Cloud SQL instances",
  "author": "Google Inc.",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/mjs/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc -p tsconfig.json && tsc -p tsconfig-esm.json",
    "prepare": "rm -rf dist && npm run compile && node ./scripts/fixup.cjs",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "test": "c8 --100 tap",
    "snap": "c8 tap",
    "presystem-test": "npm run prepare",
    "system-test": "tap --no-coverage system-test",
    "lint": "gts check",
    "fix": "gts fix",
    "posttest": "npm run lint"
  },
  "tap": {
    "coverage": false,
    "node-arg": [
      "--no-warnings",
      "--loader",
      "ts-node/esm"
    ],
    "ts": false
  },
  "devDependencies": {
    "@google-cloud/cloud-sql-connector": "file:",
    "@types/node": "^18.14.6",
    "@types/tap": "^15.0.8",
    "c8": "^7.12.0",
    "gts": "^3.1.1",
    "mysql2": "^3.2.0",
    "nock": "^13.3.0",
    "pg": "^8.10.0",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleCloudPlatform/cloud-sql-nodejs-connector"
  },
  "dependencies": {
    "@googleapis/sqladmin": "^8.0.0",
    "google-auth-library": "^8.7.0"
  }
}
